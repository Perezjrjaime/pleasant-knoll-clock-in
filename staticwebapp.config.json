{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*"]
  },
  "mimeTypes": {
    ".css": "text/css",
    ".js": "application/javascript",
    ".json": "application/json"
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' https://*.supabase.co"
  }
}